MAX_RETRY_COUNT=2

# Network Configuration
CONNECTION_TIMEOUT_SECONDS=30
RECEIVE_TIMEOUT_SECONDS=30
SEND_TIMEOUT_SECONDS=30

# Debug Configuration
ENABLE_NETWORK_LOGS=false
ENABLE_BLOC_LOGS=false
ENABLE_PERFORMANCE_LOGS=false
