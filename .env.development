# Production Environment Configuration

# API Configuration
API_BASE_URL=https://reqres.in/api
API_KEY=reqres-free-v1
API_VERSION=v1

# App Configuration
APP_NAME=ConnectX
APP_VERSION=1.0.0
ENVIRONMENT=production

# Feature Flags
ENABLE_LOGGING=false
ENABLE_ANALYTICS=true
